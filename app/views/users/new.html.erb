<%= form_for :user, url: '/users' do |f| %>
<%= f.label "名前" %>
<%= f.text_field :name, :class => "form-control" %>
<%= f.label "メールアドレス" %>
<%= f.text_field :email, :class => "form-control" %>
<%=f.label "画像をアップロード" %>
<%= f.file_field :image, :class => "form-control-file" %>
<%= f.submit value:"新規登録", :class => "btn btn-primary" %>
<% end %>